<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.meirengu.uc.dao.CitysMapper" >
    <!-- 根据省id来查询所对应的城市 -->
    <select id="showCityListByPid" parameterType="java.lang.Integer" resultType="com.meirengu.uc.model.Citys">
       select * from city where father=#{father}
    </select>
</mapper>